{
  "name": "seinjs-vscode-extension",
  "displayName": "Sein.js",
  "description": "VSCode extension for Sein.js",
  "version": "0.8.0",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:seinjs.createClass"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "seinjs.createClass",
        "title": "New Class for Sein.js",
        "category": "Sein"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "seinjs.createClass",
          "group": "navigation"
        }
      ]
    }
  },
  "configuration": {
    "type": "object",
    "title": "Sein.js",
    "properties": {
      "seinjs.fields.author": {
        "type": "string",
        "description": "Custom fields: author"
      },
      "seinjs.fields.email": {
        "type": "string",
        "description": "Custom fields: email"
      },
      "seinjs.fields.custom": {
        "type": "string",
        "description": "Custom fields: custom",
        "default": ""
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "seinjs": "^1.1.12",
    "seinjs-cli": "^0.8.22",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  }
}