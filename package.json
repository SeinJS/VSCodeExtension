{
  "name": "seinjs",
  "displayName": "Sein.js",
  "description": "VSCode extension for Sein.js",
  "version": "0.6.0",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Sein.js"
  ],
  "activationEvents": [
    "*",
    "onCommand:seinjs.createClass"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "seinjs.createClass",
        "title": "New Class for Sein.js",
        "category": "Sein"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "seinjs.createClass",
          "group": "navigation"
        }
      ]
    }
  },
  "configuration": {
    "type": "object",
    "title": "Sein.js",
    "properties": {
      "seinjs.fields.author": {
        "type": "string",
        "description": "Custom field in generated file: __author__"
      },
      "seinjs.fields.email": {
        "type": "string",
        "description": "Custom field in generated file: __email__"
      },
      "seinjs.fields.custom": {
        "type": "string",
        "description": "Custom field in generated file: __custom__",
        "default": ""
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "seinjs": "^1.1.14",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "seinjs-cli": "^0.8.22"
  }
}
